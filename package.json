  "dependencies": {
    "goodreads-api-node": "0.0.9",
    "react": "^15.4.1",
    "react-autosuggest": "^9.0.0",
    "react-bootstrap": "^0.30.7",
    "react-dom": "^15.4.1",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0",
    "redux-devtools-extension": "^2.13.0",
    "redux-logger": "^2.8.0",
    "redux-thunk": "^2.2.0",
